<div class="card mb-4">
  <div class="card-header">
    <h5 class="card-title mb-0 h6">
      Basic Information
    </h5>
  </div>


  <div class="row">
    <div class="col-md-6">
      <div class="field">
        <%= f.label :name, class:"form-label" %>
        <%= f.text_field :name, class: 'form-control' %>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-md-6">
      <div class="field">
        <%= f.label :code, class:"form-label" %>
        <%= f.text_field :code, class: 'form-control' %>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-md-6">
      <div class="field">
        <%= f.label :url, class:"form-label" %>
        <%= f.text_field :url, class: 'form-control' %>
      </div>
    </div>
  </div>
</div>